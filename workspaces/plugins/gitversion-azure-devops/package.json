{
  "name": "yarn-plugin-gitversion-azure-devops",
  "main": "./src/index.ts",
  "dependencies": {
    "@yarnpkg/cli": "^3",
    "@yarnpkg/core": "^3",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "conventional-changelog-presets-loader": "workspace:^",
    "yarn-plugin-gitversion": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "typescript": "^5.0.4"
  },
  "version": "0.0.0",
  "publishConfig": {
    "main": "./lib/index.js",
    "typings": "./lib/index.d.ts"
  },
  "files": [
    "/bundles/**/*",
    "/lib/**/*"
  ],
  "yarn.build": {
    "output": "lib"
  },
  "scripts": {
    "build": "yarn tsc --noEmit && yarn local build",
    "release": "yarn local publish",
    "build:prod": "yarn tsc --noEmit && yarn local build --production"
  }
}
